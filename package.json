    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "less": "^4.4.1",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-modules": "^5.0.0",
    "terser": "5.44.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.5"
